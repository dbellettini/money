{
    "name": "dbellettini/money",
    "description": "Yet another PHP Money library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Bellettini",
            "email": "davide@bellettini.eu"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpstan/phpstan": "^0.12.69",
        "phpstan/phpstan-phpunit": "^0.12.17",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Sb1\\Money\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sb1\\Money\\Tests\\": "tests/"
        }
    }
}
